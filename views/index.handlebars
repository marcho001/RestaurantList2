{{#each list}}

<a href="/restaurant/{{this.id}}" class="card col-sm-4 col-lg-3 m-5 shadow text-dark"
  style="min-width: 320px; text-decoration: none;">
  <img src='{{this.image}}' class="card-img-top mt-3 rounded" alt="image" style="max-height: 210px;">
  <div class="card-body p-3">
    <p class="card-text mb-4">
      <span class="mr-2 p-2 rounded-pill border border-warning text-muted shadow-sm">
        <svg class="bi bi-award" width="1em" height="1em" viewBox="0 0 16 16" fill="currentColor"
          xmlns="http://www.w3.org/2000/svg">
          <path fill-rule="evenodd"
            d="M9.669.864L8 0 6.331.864l-1.858.282-.842 1.68-1.337 1.32L2.6 6l-.306 1.854 1.337 1.32.842 1.68 1.858.282L8 12l1.669-.864 1.858-.282.842-1.68 1.337-1.32L13.4 6l.306-1.854-1.337-1.32-.842-1.68L9.669.864zm1.196 1.193l-1.51-.229L8 1.126l-1.355.702-1.51.229-.684 1.365-1.086 1.072L3.614 6l-.25 1.506 1.087 1.072.684 1.365 1.51.229L8 10.874l1.356-.702 1.509-.229.684-1.365 1.086-1.072L12.387 6l.248-1.506-1.086-1.072-.684-1.365z"
            clip-rule="evenodd" />
          <path d="M4 11.794V16l4-1 4 1v-4.206l-2.018.306L8 13.126 6.018 12.1 4 11.794z" />
        </svg>
        {{this.rating}}
      </span>
      <span class="rounded-pill bg-light border border-danger text-muted shadow-sm p-2">
        <svg class="bi bi-bookmarks mr-1" width="1em" height="1em" viewBox="0 0 16 16" fill="currentColor"
          xmlns="http://www.w3.org/2000/svg">
          <path fill-rule="evenodd"
            d="M7 13l5 3V4a2 2 0 00-2-2H4a2 2 0 00-2 2v12l5-3zm-4 1.234l4-2.4 4 2.4V4a1 1 0 00-1-1H4a1 1 0 00-1 1v10.234z"
            clip-rule="evenodd" />
          <path d="M14 14l-1-.6V2a1 1 0 00-1-1H4.268A2 2 0 016 0h6a2 2 0 012 2v12z" />
        </svg>
        {{this.category}}
      </span>
    </p>
    <p class="card-text h5">{{this.name}}
    </p>

    <p class="card-text mt-4 mb-2">
      <svg class="bi bi-phone mr-1" width="1.2em" height="1.2em" viewBox="0 0 16 16" fill="currentColor"
        xmlns="http://www.w3.org/2000/svg">
        <path fill-rule="evenodd"
          d="M11 1H5a1 1 0 00-1 1v12a1 1 0 001 1h6a1 1 0 001-1V2a1 1 0 00-1-1zM5 0a2 2 0 00-2 2v12a2 2 0 002 2h6a2 2 0 002-2V2a2 2 0 00-2-2H5z"
          clip-rule="evenodd" />
        <path fill-rule="evenodd" d="M8 14a1 1 0 100-2 1 1 0 000 2z" clip-rule="evenodd" />
      </svg>
      {{this.phone}}
    </p>
    <p class="card-text">
      <svg class="bi bi-map mr-1" width="1.2em" height="1.2em" viewBox="0 0 16 16" fill="currentColor"
        xmlns="http://www.w3.org/2000/svg">
        <path fill-rule="evenodd"
          d="M15.817.613A.5.5 0 0116 1v13a.5.5 0 01-.402.49l-5 1a.502.502 0 01-.196 0L5.5 14.51l-4.902.98A.5.5 0 010 15V2a.5.5 0 01.402-.49l5-1a.5.5 0 01.196 0l4.902.98 4.902-.98a.5.5 0 01.415.103zM10 2.41l-4-.8v11.98l4 .8V2.41zm1 11.98l4-.8V1.61l-4 .8v11.98zm-6-.8V1.61l-4 .8v11.98l4-.8z"
          clip-rule="evenodd" />
      </svg>
      {{this.location}}
    </p>
    <p class="card-text"></p>
    <p class="card-text"></p>
  </div>
</a>

{{/each}}